r(props) {\n    return(\n    <div>\n        <div class=\"red\">\n        </div>\n        <div class=\"yellow\">\n        </div>\n    <div class=\"sidenav\">\n        <a href=\"#\">item1</a>\n        <a href=\"#\">item2</a>\n        <a href=\"#\">item3</a>\n        <a href=\"#\">item4</a>\n    </div>\n \n    </div>\n)\n}","/Users/leqaajamal/Desktop/milestone-2-Team-15/milestone-2-team-15/src/components/sidebarhodhome.js",[],"/Users/leqaajamal/Desktop/milestone-2-Team-15/milestone-2-team-15/src/pages/HR.js",["129","130","131","132","133","134"],"import React from \"react\";\n\nimport { useHistory } from \"react-router-dom\";\nfunction HR() {\n  const history = useHistory();\n  function onclick() {\n    history.push(\"/Locations\");\n    return;\n  }\n\n  return (\n    <div>\n      <div class=\"red\"></div>\n      <div class=\"yellow\"></div>\n      <div class=\"sidenav\">\n        <a href=\"/HR/RegisterStaff\">Register Staff</a>\n\n        <a href=\"/HR/UpdateStaff\">Update Staff</a>\n        <a href=\"/HR/DeleteStaff\">Delete Staff</a>\n\n        <a href=\"#\">Add Signin/signout</a>\n        <a href=\"#\">view any staff attendance record</a>\n        <a href=\"#\">view staff with missing hours </a>\n        <a href=\"#\">view staff with missing days</a>\n        <a href=\"#\">Update the salary of a staff member</a>\n      </div>\n    </div>\n  );\n}\n\nexport default HR;\n","/Users/leqaajamal/Desktop/milestone-2-Team-15/milestone-2-team-15/src/components/RegisterStaff.js",["135","136","137","138","139","140","141"],"/Users/leqaajamal/Desktop/milestone-2-Team-15/milestone-2-team-15/src/pages/Location.js",["142","143","144","145"],"import React, { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { useHistory } from \"react-router-dom\";\n\nimport { ListGroup, Button, Card, Row, Col } from \"react-bootstrap\";\nimport * as Icon from \"react-bootstrap-icons\";\nimport LocationChanges from \"./LocationChanges\";\nimport { Plus } from \"react-bootstrap-icons\";\nrequire(\"dotenv\").config();\nfunction Location() {\n  const history = useHistory();\n\n  const [flag, setFlag] = useState(false);\n  const [locationsCards, setLocationsCards] = useState();\n  const [locations, setLocations] = useState();\n  const [visibility, setVisibility] = useState(false);\n  const [id, setId] = useState(0);\n  const [edit, setEdit] = useState(false);\n  useEffect(() => {\n    async function fetchData() {\n      const response = await axios.get(\n        `http://localhost:3000/HR/ViewLocations`\n      );\n      console.log(response.data);\n\n      const hi = response.data.map((location) => {\n        return (\n          <Col class=\"Location_Col\">\n            <Card>\n              <Card.Body>\n                <Card.Title>\n                  {location.BuildingCharachter + location.roomNumber}\n                </Card.Title>\n              </Card.Body>\n              <ListGroup variant=\"flush\">\n                <ListGroup.Item>\n                  {\"Floor Number :\" + location.FloorNumber}\n                </ListGroup.Item>\n                <ListGroup.Item>\n                  {\"Number Of Persons:\" + location.NumberOfPersons}\n                </ListGroup.Item>\n                <ListGroup.Item>\n                  {\"Number Of Available People:\" +\n                    location.NumberOfAvailablePeople}\n                </ListGroup.Item>\n              </ListGroup>\n              <Card.Body style={{ textAlign: \"center\" }}>\n                <Button class=\"Location__Button__delete\">\n                  <Icon.Trash />\n                </Button>\n\n                <Button\n                  onClick={() => {\n                    setId(location.locationId);\n                    setEdit(true);\n                    setVisibility(true);\n                  }}\n                  class=\"Location__Button__Edit\"\n                >\n                  <Icon.PencilSquare />\n                </Button>\n              </Card.Body>\n            </Card>\n          </Col>\n        );\n      });\n      hi.push(\n        <Col class=\"Location__Col\">\n          <Card>\n            <Button\n              onClick={() => {\n                setVisibility(true);\n              }}\n              style={{ textAlign: \"center\" }}\n            >\n              <Icon.Plus size={96} />\n            </Button>\n          </Card>\n        </Col>\n      );\n      setLocationsCards(hi);\n      setLocations(response.data);\n    }\n    fetchData();\n  }, [flag]);\n\n  return (\n    <div class=\"Hr-Buttons\">\n      <Row xs={1} sm={2} md={3} lg={4} xl={5} noGutter>\n        {locationsCards}\n      </Row>\n      <LocationChanges id={id} edit={edit} visibility={visibility} />\n    </div>\n  );\n}\n\nexport default Location;\n","/Users/leqaajamal/Desktop/milestone-2-Team-15/milestone-2-team-15/src/components/DeleteStaff.js",["146","147","148","149","150","151","152","153","154"],"/Users/leqaajamal/Desktop/milestone-2-Team-15/milestone-2-team-15/src/components/UpdateStaff.js",["155","156","157","158","159","160","161","162"],"import React, { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { useHistory } from \"react-router-dom\";\n\nimport {\n  ListGroup,\n  Form,\n  Modal,\n  Button,\n  Card,\n  Row,\n  Col,\n  Dropdown,\n} from \"react-bootstrap\";\nimport * as Icon from \"react-bootstrap-icons\";\n\nimport { Plus } from \"react-bootstrap-icons\";\nrequire(\"dotenv\").config();\nfunction RegisterStaff(props) {\n  const [type, setType] = useState();\n  const [name, setName] = useState(\"\");\n  const [email, setEmail] = useState(0);\n  const [salary, setSalary] = useState(0);\n  const [location, setLocation] = useState(0);\n  const [staff, setStaff] = useState(0);\n  const [staffArray, setStaffArray] = useState(0);\n  const [flag, setFlag] = useState(false);\n\n  useEffect(() => {\n    async function fetchData() {\n      const response = await axios.get(`http://localhost:3000/HR/ViewStaffs`);\n      co